class Main {
    public static void main(String[] args) {
        
       int arr[]={1,2,7,10,4,8,5,6,4};
       int max=arr[0];
        for(int i=1; i<arr.length; i++) {
            if(arr[i] > max) {   
                max = arr[i];  
            }
            
            
        }
        System.out.println("max: " + max);
    }
}

// Second Largest: 10